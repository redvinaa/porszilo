<?xml version="1.0"?>
<launch>

  <arg name="base_global_planner" default="navfn/NavfnROS"/>
	<arg name="base_local_planner"  default="dwa_local_planner/DWAPlannerROS"/>
	<arg name="move_base_map_topic" default="/map"/>
  
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="log" required="true">
    <remap from="cmd_vel" to="nav_vel" /><!--to="mobile_base_controller/cmd_vel"-->
		<remap from="/map"     to="$(arg move_base_map_topic)"/>
		<!-- <remap from="/map"     to="$(arg move_base_map_topic)"/> -->
		<remap from="odom"    to="/odometry/filtered"/>
		<remap from="/move_base_simple/goal" to="porszilo/move_base/goal"/>

    <param name="base_global_planner" value="$(arg base_global_planner)"/>
    <param name="base_local_planner"  value="$(arg base_local_planner)"/>  

	  <rosparam file="$(find porszilo_navigation)/config/move_base_params.yaml"      command="load"/>
	  <rosparam file="$(find porszilo_navigation)/config/costmap_common_params.yaml" command="load"/>
	  <rosparam file="$(find porszilo_navigation)/config/local_costmap_params.yaml"  command="load"/>
	  <rosparam file="$(find porszilo_navigation)/config/global_costmap_params.yaml" command="load"/>
  </node>

</launch>
